<div id="<%= dom_id bank_account %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= bank_account.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Institution:</strong>
    <%= bank_account.institution %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= bank_account.user_id %>
  </p>

</div>
